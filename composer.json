{
    "name": "lemax10/asail",
    "description": "Based on Laravel Sail. Advanced sail.",
    "keywords": ["laravel", "docker", "octobercms"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/lemax10/asail/issues",
        "source": "https://github.com/lemax10/asail"
    },
    "authors": [
        {
            "name": "Vladimir Pyankov",
            "email": "vladimir@pyankov.pro"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/contracts": "^6.0|^7.0|^8.0|^9.0",
        "illuminate/console": "^6.0|^7.0|^8.0|^9.0",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0"
    },
    "bin": [
        "bin/sail"
    ],
    "autoload": {
        "psr-4": {
            "LeMaX10\\ASail\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "LeMaX10\\ASail\\SailServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
